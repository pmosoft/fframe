<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="$packNm$.$FileName$Dao">

<select id="select$FileName$List" parameterType="java.util.HashMap" resultType="java.util.HashMap">

	-- $FileName$Dao.select$FileName$List
	SELECT   '제목'    AS TITLE
			,'내용'    AS CONTENT
			,DATE_FORMAT(REG_DTM,'%Y.%m.%d %H:%i:%S') AS REG_DTM
			,'ADMIN'   AS REG_USR_ID
			,DATE_FORMAT(UPD_DTM,'%Y.%m.%d %H:%i:%S') AS UPD_DTM
			,''        ASUPD_USR_ID
	--FROM    DUAL
    --WHERE   PKG2_NM LIKE CONCAT(CONCAT('%',#{searchValue}),'%')
    --OR      PKG3_NM LIKE CONCAT(CONCAT('%',#{searchValue}),'%')
	--ORDER BY PKG_FUL_NM
</select>

<select id="select$FileName$Cnt" parameterType="java.util.HashMap" resultType="int">
	-- $FileName$Dao.select$FileName$Cnt
	SELECT COUNT(*) CNT
	FROM   DUAL
</select>

<insert id="insert$FileName$" parameterType="java.util.HashMap">

	-- $FileName$Dao.insert$FileName$
	INSERT INTO TDACM00010
	(
	     PKG_FUL_NM
		,PKG2_NM
		,REG_DTM
		,REG_USR_ID
		,UPD_DTM
		,UPD_USR_ID
	) VALUES (
         #{PKG_FUL_NM}
		,#{PKG2_NM}
		,curdate()
		,#{REG_USR_ID}
		,curdate()
		,#{UPD_USR_ID}
	)

</insert>

<delete id="delete$FileName$" parameterType="java.util.HashMap">
	-- $FileName$Dao.delete$FileName$
	DELETE FROM TDACM00040 WHERE COL_STS_CD = #{COL_STS_CD}
</delete>

<update id="update$FileName$" parameterType="java.util.HashMap">

	-- $FileName$Dao.update$FileName$
	UPDATE TDACM00010
	SET  PKG2_NM      = #{PKG2_NM}
		,PKG3_NM      = #{PKG3_NM}
		,PKG4_NM      = #{PKG4_NM}
		,PKG_HNM      = #{PKG_HNM}
		,PKG_DESC     = #{PKG_DESC}
		,USE_YN       = CASE WHEN #{USE_YN} = 'true' THEN 'Y' ELSE 'N' END
		,UPD_DTM      = curdate()
		,UPD_USR_ID  = #{UPD_USR_ID}
	WHERE  PKG_FUL_NM = #{PKG_FUL_NM}

</update>

</mapper>
